mp{\r\n    display: grid;\r\n    grid-template-columns: repeat(3, 2fr);\r\n    gap: 10px;\r\n    padding: 10px;\r\n    \r\n}\r\n\r\n.card{\r\n    font-family: DejaVu Sans Mono, monospace;\r\n    border: 2px solid rgb(78, 78, 78);\r\n    border-radius: 5px;\r\n    /* background: radial-gradient(circle, black, white); */\r\n    text-align: center;\r\n    background-color: black;\r\n    color: goldenrod;\r\n    box-shadow: 0 8px 16px 0 black, 0 6px 20px 0 purple;\r\n}\r\n.card:before {\r\n    content: '';\r\n    width: inherit;\r\n    height: inherit;\r\n    position: absolute;\r\n    background: #fff;\r\n    opacity: 0;\r\n    transition: opacity .1s ease-out;   \r\n  }\r\n  .card:hover:before { \r\n    opacity: 1;\r\n  }\r\n  .card .text-container {\r\n    width: inherit;\r\n    padding: 5px 15px;\r\n    color: #444;\r\n    position: absolute;\r\n    top: 100%; \r\n    display: none;\r\n  }\r\n  .card .text-container p{\r\n    color:red;\r\n  }\r\n  .card:hover .text-container {    \r\n    top: 0;\r\n    display: block;\r\n    padding-top: 50px;\r\n  }\r\n/* .card:hover h3{\r\n    display: none;\r\n}\r\n.card:hover hr{\r\n    display: none;\r\n} */\r\n.card:hover .title{\r\n    display: none;\r\n}\r\n.card:hover .front{\r\n    display: none;\r\n}\r\n.card:hover .types{\r\n    display: none;\r\n}\r\n\r\n.card:hover{\r\n    transform: scale(1.05);\r\n    background: radial-gradient(circle, white, black);\r\n    color:wheat;\r\n    animation: resize 3s infinite;\r\n    cursor: pointer;\r\n}\r\n\r\n@keyframes resize {\r\n    0% {\r\n        transform: scale(1);\r\n    }\r\n    50% {\r\n        transform: scale(1.05);\r\n    }\r\n    100% {\r\n        transform: scale(1);\r\n    }\r\n}\r\n\r\n/* .card h3{\r\n    background-image: linear-gradient(red, black);\r\n} */\r\n.types {\r\n  \r\n    background-image: linear-gradient(black, red);\r\n}\r\n/* .types:hover{\r\n    background-image: linear-gradient(white, red);\r\n} */\r\n"},"lineCount":null}},"error":null,"hash":"c2e5